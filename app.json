{
  "expo": {
    "name": "ai-hua",
    "slug": "ai-hua",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "com.aihua",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.addevelopments.aihua"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.addevelopments.aihua"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
     [ "@react-native-google-signin/google-signin",
      {
        "iosUrlScheme": "com.googleusercontent.apps.963568773459-r4rrdvfnts88bfvau69mgjg0pluqmnsb"
      }]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "daa9a248-b32d-4580-8617-ec18fc511aab"
      }
    }
  }
}
